void setup()
{
  pinMode(10, OUTPUT);
  pinMode(11, OUTPUT);
  pinMode(5, OUTPUT);
  pinMode(6, OUTPUT);
}

void loop()
{
  digitalWrite(10, HIGH);
  digitalWrite(11, LOW);
  delay(3000);
  digitalWrite(5, HIGH);
  digitalWrite(6, LOW);
  delay(3000);
  digitalWrite(11, HIGH);
  digitalWrite(10, LOW);
  delay(3000);
  digitalWrite(6, HIGH);
  digitalWrite(5, LOW);
  delay(3000);
